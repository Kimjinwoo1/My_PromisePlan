<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nepplus.my_promiseplan">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyPromisePlan"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.My_PromisePlan">
        <activity
            android:name=".settings.MyPlanList"
            android:exported="false" />
        <activity
            android:name=".settings.MyPlanActivity"
            android:exported="false" />
        <activity
            android:name=".ui.EditAppointmentActivity"
            android:exported="false" />
        <activity
            android:name=".settings.AddFriendsActivity"
            android:exported="false" />
        <activity
            android:name=".settings.MyFriendsActivity"
            android:exported="false" />
        <activity
            android:name=".main.signup.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".main.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <!-- Redirect URI: "kakao${NATIVE_APP_KEY}://oauth" -->
                <data
                    android:host="oauth"
                    android:scheme="kakao94464b925fe5735e13b18e1c8a8512a8" />
            </intent-filter>
        </activity>
    </application>

</manifest>